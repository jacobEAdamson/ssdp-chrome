{
  "manifest_version": 2,

  "name": "SSDP Provider",
  "description": "This app allows extensions and webpages to make SSDP queries",
  "version": "0.0.0.1",

  "app": {
    "background": {
	  "scripts": ["app.js"]
	}
  },
  "icons": { 
	"16": "16.png",
	"48": "48.png",
	"128": "128.png" 
  },
  "sockets": {
    "udp": {
      "send": ["*"],
      "bind": ["*"]
    }
  }
}